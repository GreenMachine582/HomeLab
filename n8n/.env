###############################################
# n8n NON-SENSITIVE CONFIG
###############################################

# Internal hostname (container name / service name)
N8N_HOST=n8n
N8N_PORT=5678
N8N_PROTOCOL=https

# Public-facing URL via Cloudflare Tunnel
N8N_WEBHOOK_URL=https://n8n.green-tech-hub.com

# Timezone
GENERIC_TIMEZONE=Australia/Sydney
TZ=Australia/Sydney

# Deprecation / behaviour flags (non-secret)
DB_SQLITE_POOL_SIZE=5
N8N_RUNNERS_ENABLED=true
N8N_BLOCK_ENV_ACCESS_IN_NODE=false
N8N_GIT_NODE_DISABLE_BARE_REPOS=true

# Logging level
N8N_LOG_LEVEL=info

# User management behaviour
N8N_USER_MANAGEMENT_DISABLED=false
