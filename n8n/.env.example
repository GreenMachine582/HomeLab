###############################################
# n8n ENV TEMPLATE
# Copy to:
#   .env        for non-secret config
#   .env.secret for secrets (gitignored)
###############################################

# ---- Non-sensitive (goes to: .env) ----
N8N_HOST=n8n
N8N_PORT=5678
N8N_PROTOCOL=https
N8N_WEBHOOK_URL=https://n8n.example.com/
WEBHOOK_URL=https://n8n.example.com/
GENERIC_TIMEZONE=Australia/Sydney
TZ=Australia/Sydney
DB_SQLITE_POOL_SIZE=5
N8N_RUNNERS_ENABLED=true
N8N_BLOCK_ENV_ACCESS_IN_NODE=false
N8N_GIT_NODE_DISABLE_BARE_REPOS=true
N8N_LOG_LEVEL=info
N8N_USER_MANAGEMENT_DISABLED=false

# ---- Sensitive (goes to: .env.secret, DO NOT COMMIT) ----
# N8N_USER=admin
# N8N_PASSWORD=change-me
# N8N_OWNER_EMAIL=admin@example.com
# N8N_OWNER_PASSWORD=change-me
# N8N_ENCRYPTION_KEY=GENERATE_32_CHAR_RANDOM
